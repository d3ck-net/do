{
  "name": "keyval",
  "displayName": "kEyVAL",
  "description": "bind custom code to keystrokes",
  "version": "1.2.0",
  "publisher": "jmanuels",
  "repository": "https://github.com/d3ck-net/keyval",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands":[
      {
        "command": "do",
        "title":"do",
        "category": "core"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Macros configuration",
      "properties": {
        "do.defaultType": {
          "type":"string",
          "default":"terminal",
          "description": "the default action type to use when passing raw strings"
        },
        "do.macros":{
          "type":"object",
          "description": "define a map of actionLists to be used inside other actions"
        },
        "do.onStart":{
          "type":"array",
          "description": "a list of actions to run on startup"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "webpack":"echo webpack"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "eslint": "^3.6.0",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
},
"dependencies": {
    "async": "^2.5.0"
  }
}
